{
  "manifest_version": 3,
  "name": "Notes Extension",
  "version": "1.0",
  "permissions": ["storage", "notifications", "contextMenus"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png"
  }
}